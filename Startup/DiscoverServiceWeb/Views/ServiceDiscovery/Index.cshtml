@Scripts.Render("~/Scripts/Index.js")
<div>
    <div class="panel-title">Service Discovery </div>
    <table class="table table-hover table-striped table-bordered">
        <thead>
            <tr>
                <td width="50%">接口名称</td>
                <td width="50%">描述</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewBag.Data)
            {
                <tr>
                    <td><a href='javascript:OpenInterfaceDetail("@(item.InterfaceName)".replace(/\./g,"-"));'>@(item.InterfaceName)</a></td>
                    <td>@(item.Description)</td>
                </tr>
            }
        </tbody>
    </table>
</div>